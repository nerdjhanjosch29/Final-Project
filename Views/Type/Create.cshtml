@model wow.Models.ViewModels.TypeViewModel

<div class="mt-3">
    <div class="row">
        <div class="col-12">
            <h2>Add Types Here</h2>
        </div>
    </div>
    <form method="post">
          <div class="form-group">
            <label asp-for="Items"></label>
            <select asp-for="Type.Itemid"asp-items="Model.selectListItem(Model.Items)" class="form-control"></select>
            
            <span asp-validation-for="Items" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Type.Name"></label>
            <input asp-for="Type.Name" type="text" class="form-control">
            <span asp-validation-for="Type.Name" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary">Save</button>
        <a asp-action="Index" class="btn btn-success">Back</a>
    </form>
</div>